{
  "name": "pyxis",
  "version": "0.12.0",
  "private": true,
  "scripts": {
    "dev": "npm run setup-build && next dev --turbopack",
    "build": "npm run setup-build && next build",
    "start": "npm run setup-build && next start",
    "setup-build": "rm -rf public/locales && cp -r locales public/locales && node initial_files_to_ts.js && node build-extensions.js && cp extensions/registry.json public/extensions/registry.json",
    "format": "bash scripts/format_locales.sh && prettier --write src/**/*.ts src/**/*.tsx",
    "lint": "eslint --cache --ext .ts,.tsx,.js,.jsx . --max-warnings=0",
    "lint:fix": "eslint --cache --ext .ts,.tsx,.js,.jsx . --fix",
    "prepare": "husky install",
    "test": "jest",
    "update": "npm i && npm run build && node scripts/update-readmes-version.js && ./tree.sh && ./tree-built-out.sh",
    "preview": "npx serve out"
  },
  "dependencies": {
